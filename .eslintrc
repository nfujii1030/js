{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "standard", "plugin:prettier/recommended"],
  "env": {
    "browser": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "semi": false,
        "singleQuote": true,
        "trailingComma": "all"
      }
    ],
    "no-console": 1,
    "prefer-const": 2,
    "no-var": 2
  }
}
